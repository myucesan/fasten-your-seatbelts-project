<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="302.0" prefWidth="600.0" style="-fx-background-image: url(&quot;file:images/photobooth-wallpaper.jpeg&quot;);" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="newphotoboothui.FXMLFotoController">
   <children>
      <ImageView fx:id="imgWebCamCapturedImage" fitHeight="250.0" fitWidth="440.0" layoutX="4.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="107.0" fitWidth="100.0" layoutX="14.0" layoutY="257.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/rotate-left.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="107.0" fitWidth="100.0" layoutX="114.0" layoutY="257.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/rotate-right.png" />
         </image>
      </ImageView>
      <Button layoutX="114.0" layoutY="258.0" mnemonicParsing="false" onAction="#cameraTurnRight" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: transparant;" />
      <Button layoutX="20.0" layoutY="258.0" mnemonicParsing="false" onAction="#cameraTurnLeft" prefHeight="100.0" prefWidth="89.0" style="-fx-background-color: transparant;" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="444.0" layoutY="107.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/take-picture.png" />
         </image>
      </ImageView>
      <Button fx:id="fotoButton" layoutX="469.0" layoutY="132.0" mnemonicParsing="false" onAction="#makePicture" style="-fx-background-radius: 1000px; -fx-min-height: 100px; -fx-min-width: 100px; -fx-background-image: transparant; -fx-background-position: center; -fx-background-size: 100% 100%; -fx-background-color: transparant;" />
   </children>
</AnchorPane>
